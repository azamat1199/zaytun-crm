{
    "name": "crm-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "env-cmd -f .env.dev next dev",
        "dev-local": "env-cmd -f .env.local next dev",
        "build": "next build",
        "start": "next start",
        "test": "vitest",
        "test-unwatch": "vitest --watch=false && echo 'Tests successfully passed '",
        "link-local-lib": "yarn link @zaytun/components && yarn link react && yarn unlink react-dom && yarn add link:../frontend-components",
        "unlink-local-lib": "yarn remove @zaytun/components && yarn unlink @zaytun/components && yarn unlink react && yarn unlink react-dom && yarn add @zaytun/components",
        "lint": "next lint",
        "lint-error": "eslint . --quiet",
        "format": "prettier --check --ignore-path .gitignore .",
        "format-fix": "prettier --write --ignore-path .gitignore .",
        "lint-staged": "lint-staged",
        "type-check": "tsc --project tsconfig.json --pretty --noEmit && echo ",
        "unused-packages": "npx depcheck",
        "prepare": "husky",
        "storybook": "concurrently yarn:watch:*",
        "watch:tailwind": "npx tailwindcss -i ./src/app/[locale]/globals.css --watch",
        "watch:storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
